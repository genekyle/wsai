{
  "version": "1.0.0",
  "manifest_version": 2,
  "name": "Proxy Authentication",
  "permissions": [
      "proxy",
      "tabs",
      "unlimitedStorage",
      "storage",
      "<all_urls>",
      "webRequest",
      "webRequestBlocking"
  ],
  "background": {
      "scripts": ["background.js"]
  }
}
